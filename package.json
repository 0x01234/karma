    "jquery-bridget": "^2.0.1",
    "less": "^2.7.2",
    "mock-xhr": "^0.1.0",
    "node-sass": "^4.5.3",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "window": "^4.2.0"
  }
